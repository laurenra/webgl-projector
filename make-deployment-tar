echo "Save files to deploy in webgl-projector.tar."
# Start from parent directory to include the webgl-projector directory in the tarball
cd ..
tar cvf webgl-projector/webgl-projector.tar webgl-projector/js/controls/DragControls.js
tar rvf webgl-projector/webgl-projector.tar webgl-projector/js/controls/OrbitControls.js
tar rvf webgl-projector/webgl-projector.tar webgl-projector/js/controls/TrackballControls.js

tar rvf webgl-projector/webgl-projector.tar webgl-projector/js/libs/Detector.js
tar rvf webgl-projector/webgl-projector.tar webgl-projector/js/libs/jquery-3.3.1.min.js
tar rvf webgl-projector/webgl-projector.tar webgl-projector/js/libs/modernizr-custom.min.js
tar rvf webgl-projector/webgl-projector.tar webgl-projector/js/libs/stats.min.js
tar rvf webgl-projector/webgl-projector.tar webgl-projector/js/libs/three.min.js

tar rvf webgl-projector/webgl-projector.tar webgl-projector/js/loaders/ColladaLoader.js
tar rvf webgl-projector/webgl-projector.tar webgl-projector/js/loaders/OBJLoader.js

tar rvf webgl-projector/webgl-projector.tar webgl-projector/js/projector.js

tar rvf webgl-projector/webgl-projector.tar webgl-projector/models/collada/Projector1.dae
tar rvf webgl-projector/webgl-projector.tar webgl-projector/models/collada/ProjectorScreen120inch.dae

tar rvf webgl-projector/webgl-projector.tar webgl-projector/styles/projector.css

tar rvf webgl-projector/webgl-projector.tar webgl-projector/textures/Wood_Floor_007_COLOR.jpg

tar rvf webgl-projector/webgl-projector.tar webgl-projector/projector.html

# Return to the webgl-projector directory
cd webgl-projector
